}

;; testable
(tuple) fibonacci_sequence (int n, int k) method_id 
{
  if (k == 0)
  {
    return empty_tuple();
  }
  n -= 2;
  int previous = 0;
  int current = 1;

  repeat (n)
  {
    (previous, current) = (current, previous + current);
  }

  tuple list = empty_tuple();
  repeat (0 - n)
  {
    n += 1;
    k -= 1;
    list~tpush(n + 1);
  }

  repeat (k)
  {
    (previous, current) = (current, previous + current);
    list~tpush(current);
  }

  return list;
}
