forall X -> (tuple) to_tuple (X x) asm "NOP";

;; testable
(tuple) fibonacci_sequence (int n, int k) method_id
{
  if ((k <= 0) | (n < 0) | (n + k > 371) | (n > 371) | (k > 255))
  {
    return empty_tuple();
  }

  var p = 0;
  var c = 1;
  var list = empty_tuple();

  repeat (n)
  {
    var t = p + c;
    p = c;
    c = t;
  }

  repeat (k)
  {
    list~tpush(p);
    var t = p + c;
    p = c;
    c = t;
  }
  
  return list;
}