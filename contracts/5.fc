}

;; testable
(tuple) fibonacci_sequence (int n, int k) method_id
{
  if (k == 0)
  {
    return empty_tuple();
  }
  
  var previous = 0;
  var current = 1;
  var list = empty_tuple();

  repeat (n - 2){
    (previous, current) = (current, previous + current);
  }
  
  n -= 2;
  repeat (0 - n){
    n += 1;
    k -= 1;
    list~tpush(n + 1);
  }

  repeat (k){
    (previous, current) = (current, previous + current);
    list~tpush(current);
  }

  return list;
}