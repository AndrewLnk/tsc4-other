() recv_internal() 
{
}

;; testable
(cell) find_branch_by_hash(int hash, cell tree) method_id 
{
  tuple stack = null();
  stack~push_back(tree);

  while (stack.is_null() == 0)
  {
    var c = stack~pop_back();

    if (cell_hash(c) == hash)
    {
      return c;
    }

    var s = c.begin_parse();
    repeat (s.slice_refs()) 
    {
      stack~push_back(s~load_ref());
    }
  }

  return begin_cell().end_cell();
}
